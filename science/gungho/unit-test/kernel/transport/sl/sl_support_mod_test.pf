  @test
  subroutine compute_quintic_coeffs_test()
    use funit
    use, intrinsic :: iso_fortran_env,  only: real64
    use sl_support_mod,                 only: compute_quintic_coeffs

    implicit none

    real(kind=r_tran)   :: z_grid(1:6), z_interp(1:6), dz(1:6), tol
    real(kind=r_tran)   :: cq(1:6,1:6)
    integer(kind=i_def) :: sq(1:6,1:6)
    integer(kind=i_def) :: ni, ng

    ! Set tolerance
    if ( r_tran == real64 ) then
      tol = 1.0e-12_r_tran
    else
      tol = 1.0e-6_r_tran
    end if

    ! Parameters
    ni = 6_i_def
    ng = 6_i_def

    ! Set up cell centres on uniform grid
    dz(1:6)=1.0_r_tran
    z_grid(1) = 0.5_r_tran
    z_grid(2) = 1.5_r_tran
    z_grid(3) = 2.5_r_tran
    z_grid(4) = 3.5_r_tran
    z_grid(5) = 4.5_r_tran
    z_grid(6) = 5.5_r_tran

    ! Set interpolated location
    z_interp(1) = 0.5_r_tran
    z_interp(2) = 0.8_r_tran
    z_interp(3) = 3.1_r_tran
    z_interp(4) = 4.2_r_tran
    z_interp(5) = 5.3_r_tran
    z_interp(6) = 5.5_r_tran

    ! Run subroutine
    call compute_quintic_coeffs(z_interp,z_grid,dz,sq,cq,ni,ng)

    ! Test the indices
    @assertEqual(1.0_r_tran, real(sq(1,1),r_tran), tol)
    @assertEqual(1.0_r_tran, real(sq(2,1),r_tran), tol)
    @assertEqual(1.0_r_tran, real(sq(3,1),r_tran), tol)
    @assertEqual(2.0_r_tran, real(sq(4,1),r_tran), tol)
    @assertEqual(3.0_r_tran, real(sq(5,1),r_tran), tol)
    @assertEqual(4.0_r_tran, real(sq(6,1),r_tran), tol)
    @assertEqual(1.0_r_tran, real(sq(1,2),r_tran), tol)
    @assertEqual(1.0_r_tran, real(sq(2,2),r_tran), tol)
    @assertEqual(1.0_r_tran, real(sq(3,2),r_tran), tol)
    @assertEqual(2.0_r_tran, real(sq(4,2),r_tran), tol)
    @assertEqual(3.0_r_tran, real(sq(5,2),r_tran), tol)
    @assertEqual(4.0_r_tran, real(sq(6,2),r_tran), tol)
    @assertEqual(1.0_r_tran, real(sq(1,3),r_tran), tol)
    @assertEqual(2.0_r_tran, real(sq(2,3),r_tran), tol)
    @assertEqual(3.0_r_tran, real(sq(3,3),r_tran), tol)
    @assertEqual(4.0_r_tran, real(sq(4,3),r_tran), tol)
    @assertEqual(5.0_r_tran, real(sq(5,3),r_tran), tol)
    @assertEqual(6.0_r_tran, real(sq(6,3),r_tran), tol)
    @assertEqual(2.0_r_tran, real(sq(1,4),r_tran), tol)
    @assertEqual(3.0_r_tran, real(sq(2,4),r_tran), tol)
    @assertEqual(4.0_r_tran, real(sq(3,4),r_tran), tol)
    @assertEqual(5.0_r_tran, real(sq(4,4),r_tran), tol)
    @assertEqual(6.0_r_tran, real(sq(5,4),r_tran), tol)
    @assertEqual(6.0_r_tran, real(sq(6,4),r_tran), tol)
    @assertEqual(3.0_r_tran, real(sq(1,5),r_tran), tol)
    @assertEqual(4.0_r_tran, real(sq(2,5),r_tran), tol)
    @assertEqual(5.0_r_tran, real(sq(3,5),r_tran), tol)
    @assertEqual(6.0_r_tran, real(sq(4,5),r_tran), tol)
    @assertEqual(6.0_r_tran, real(sq(5,5),r_tran), tol)
    @assertEqual(6.0_r_tran, real(sq(6,5),r_tran), tol)
    @assertEqual(3.0_r_tran, real(sq(1,6),r_tran), tol)
    @assertEqual(4.0_r_tran, real(sq(2,6),r_tran), tol)
    @assertEqual(5.0_r_tran, real(sq(3,6),r_tran), tol)
    @assertEqual(6.0_r_tran, real(sq(4,6),r_tran), tol)
    @assertEqual(6.0_r_tran, real(sq(5,6),r_tran), tol)
    @assertEqual(6.0_r_tran, real(sq(6,6),r_tran), tol)

    ! Test the quintic coefficients
    @assertEqual(0.0_r_tran, cq(1,1), tol)
    @assertEqual(0.0_r_tran, cq(2,1), tol)
    @assertEqual(1.0_r_tran, cq(3,1), tol)
    @assertEqual(0.0_r_tran, cq(4,1), tol)
    @assertEqual(0.0_r_tran, cq(5,1), tol)
    @assertEqual(0.0_r_tran, cq(6,1), tol)
    @assertEqual(0.0_r_tran, cq(1,2), tol)
    @assertEqual(0.0_r_tran, cq(2,2), tol)
    @assertEqual(0.7_r_tran, cq(3,2), tol)
    @assertEqual(0.3_r_tran, cq(4,2), tol)
    @assertEqual(0.0_r_tran, cq(5,2), tol)
    @assertEqual(0.0_r_tran, cq(6,2), tol)
    @assertEqual(0.010752_r_tran, cq(1,3), tol)
    @assertEqual(-0.08736_r_tran, cq(2,3), tol)
    @assertEqual(0.46592_r_tran,  cq(3,3), tol)
    @assertEqual(0.69888_r_tran,  cq(4,3), tol)
    @assertEqual(-0.09984_r_tran, cq(5,3), tol)
    @assertEqual(0.011648_r_tran, cq(6,3), tol)
    @assertEqual(0.0_r_tran,     cq(1,4), tol)
    @assertEqual(-0.0455_r_tran, cq(2,4), tol)
    @assertEqual(0.3315_r_tran,  cq(3,4), tol)
    @assertEqual(0.7735_r_tran,  cq(4,4), tol)
    @assertEqual(-0.0595_r_tran, cq(5,4), tol)
    @assertEqual(0.0_r_tran, cq(6,4), tol)
    @assertEqual(0.0_r_tran, cq(1,5), tol)
    @assertEqual(0.0_r_tran, cq(2,5), tol)
    @assertEqual(0.2_r_tran, cq(3,5), tol)
    @assertEqual(0.8_r_tran, cq(4,5), tol)
    @assertEqual(0.0_r_tran, cq(5,5), tol)
    @assertEqual(0.0_r_tran, cq(6,5), tol)
    @assertEqual(0.0_r_tran, cq(1,6), tol)
    @assertEqual(0.0_r_tran, cq(2,6), tol)
    @assertEqual(0.0_r_tran, cq(3,6), tol)
    @assertEqual(1.0_r_tran, cq(4,6), tol)
    @assertEqual(0.0_r_tran, cq(5,6), tol)
    @assertEqual(0.0_r_tran, cq(6,6), tol)

  end subroutine compute_quintic_coeffs_test

