  @test
  subroutine monotone_quintic_sl_test()
    use funit
    use, intrinsic :: iso_fortran_env,  only: real64
    use sl_support_mod,                 only: monotone_quintic_sl,             &
                                              compute_quintic_coeffs,          &
                                              compute_linear_coeffs
    use transport_enumerated_types_mod, only: vertical_monotone_strict,        &
                                              vertical_monotone_relaxed,       &
                                              vertical_monotone_order_linear,  &
                                              vertical_monotone_order_high,    &
                                              vertical_monotone_order_constant

    implicit none

    real(kind=r_tran)   :: z_grid(1:6), z_interp(1:6), dz(1:6), tol
    real(kind=r_tran)   :: f_grid(1:6), f_interp(1:6), f_interp_test(1:6)
    real(kind=r_tran)   :: cc(1:6,1:6), cl(1:2,1:6)
    integer(kind=i_def) :: sc(1:6,1:6)
    integer(kind=i_def) :: ni, ng
    integer(kind=i_def) :: monotone, monotone_order

    ! Set tolerance
    if ( r_tran == real64 ) then
      tol = 1.0e-12_r_tran
    else
      tol = 1.0e-6_r_tran
    end if

    ! Parameters
    ni = 6_i_def
    ng = 6_i_def

    ! Set up cell centres on uniform grid
    dz(1:6)=1.0_r_tran
    z_grid(1) = 0.5_r_tran
    z_grid(2) = 1.5_r_tran
    z_grid(3) = 2.5_r_tran
    z_grid(4) = 3.5_r_tran
    z_grid(5) = 4.5_r_tran
    z_grid(6) = 5.5_r_tran

    ! Set interpolated location
    z_interp(1) = 0.5_r_tran
    z_interp(2) = 0.8_r_tran
    z_interp(3) = 3.1_r_tran
    z_interp(4) = 4.2_r_tran
    z_interp(5) = 5.3_r_tran
    z_interp(6) = 5.5_r_tran

    ! Get quintic and linear coefficients
    call compute_quintic_coeffs(z_interp,z_grid,dz,sc,cc,ni,ng)
    call compute_linear_coeffs(z_interp,z_grid,dz,cl,ni,ng)

    ! Set up grid data
    f_grid(1) = 4.0_r_tran
    f_grid(2) = 1.0_r_tran
    f_grid(3) = 4.0_r_tran
    f_grid(4) = 4.0_r_tran
    f_grid(5) = 1.0_r_tran
    f_grid(6) = 1.0_r_tran

    ! Set up interpolated data
    f_interp(1) =  cc(1,1)*f_grid(sc(1,1))+cc(2,1)*f_grid(sc(2,1)) &
                 + cc(3,1)*f_grid(sc(3,1))+cc(4,1)*f_grid(sc(4,1)) &
                 + cc(5,1)*f_grid(sc(5,1))+cc(6,1)*f_grid(sc(6,1))
    f_interp(2) =  cc(1,2)*f_grid(sc(1,2))+cc(2,2)*f_grid(sc(2,2)) &
                 + cc(3,2)*f_grid(sc(3,2))+cc(4,2)*f_grid(sc(4,2)) &
                 + cc(5,2)*f_grid(sc(5,2))+cc(6,2)*f_grid(sc(6,2))
    f_interp(3) =  cc(1,3)*f_grid(sc(1,3))+cc(2,3)*f_grid(sc(2,3)) &
                 + cc(3,3)*f_grid(sc(3,3))+cc(4,3)*f_grid(sc(4,3)) &
                 + cc(5,3)*f_grid(sc(5,3))+cc(6,3)*f_grid(sc(6,3))
    f_interp(4) =  cc(1,4)*f_grid(sc(1,4))+cc(2,4)*f_grid(sc(2,4)) &
                 + cc(3,4)*f_grid(sc(3,4))+cc(4,4)*f_grid(sc(4,4)) &
                 + cc(5,4)*f_grid(sc(5,4))+cc(6,4)*f_grid(sc(6,4))
    f_interp(5) =  cc(1,5)*f_grid(sc(1,5))+cc(2,5)*f_grid(sc(2,5)) &
                 + cc(3,5)*f_grid(sc(3,5))+cc(4,5)*f_grid(sc(4,5)) &
                 + cc(5,5)*f_grid(sc(5,5))+cc(6,5)*f_grid(sc(6,5))
    f_interp(6) =  cc(1,6)*f_grid(sc(1,6))+cc(2,6)*f_grid(sc(2,6)) &
                 + cc(3,6)*f_grid(sc(3,6))+cc(4,6)*f_grid(sc(4,6)) &
                 + cc(5,6)*f_grid(sc(5,6))+cc(6,6)*f_grid(sc(6,6))
    ! Strict monotonicity should set the index 3 value to 4
    ! and leave the others unchanged for all orders

    ! Test strict monotone and constant order -------------------------
    monotone = vertical_monotone_strict
    monotone_order = vertical_monotone_order_constant

    ! Run subroutine to test
    f_interp_test = f_interp
    call monotone_quintic_sl(f_interp_test,f_grid,sc,cl,monotone,monotone_order,1,6)

    ! Test the interpolated values
    @assertEqual(4.0_r_tran, f_interp_test(1), tol)
    @assertEqual(3.1_r_tran, f_interp_test(2), tol)
    @assertEqual(4.0_r_tran, f_interp_test(3), tol)
    @assertEqual(1.858_r_tran, f_interp_test(4), tol)
    @assertEqual(1.0_r_tran, f_interp_test(5), tol)
    @assertEqual(1.0_r_tran, f_interp_test(6), tol)

    ! Test strict monotone and linear order -------------------------
    monotone = vertical_monotone_strict
    monotone_order = vertical_monotone_order_linear

    ! Run subroutine to test
    f_interp_test = f_interp
    call monotone_quintic_sl(f_interp_test,f_grid,sc,cl,monotone,monotone_order,1,6)

    ! Test the interpolated values
    @assertEqual(4.0_r_tran, f_interp_test(1), tol)
    @assertEqual(3.1_r_tran, f_interp_test(2), tol)
    @assertEqual(4.0_r_tran, f_interp_test(3), tol)
    @assertEqual(1.858_r_tran, f_interp_test(4), tol)
    @assertEqual(1.0_r_tran, f_interp_test(5), tol)
    @assertEqual(1.0_r_tran, f_interp_test(6), tol)

    ! Test strict monotone and high order -------------------------
    monotone = vertical_monotone_strict
    monotone_order = vertical_monotone_order_high

    ! Run subroutine to test
    f_interp_test = f_interp
    call monotone_quintic_sl(f_interp_test,f_grid,sc,cl,monotone,monotone_order,1,6)

    ! Test the interpolated values
    @assertEqual(4.0_r_tran, f_interp_test(1), tol)
    @assertEqual(3.1_r_tran, f_interp_test(2), tol)
    @assertEqual(4.0_r_tran, f_interp_test(3), tol)
    @assertEqual(1.858_r_tran, f_interp_test(4), tol)
    @assertEqual(1.0_r_tran, f_interp_test(5), tol)
    @assertEqual(1.0_r_tran, f_interp_test(6), tol)

    ! Test relaxed monotone and constant order -------------------------
    ! Relaxed monotonicity should leave the values unchanged
    monotone = vertical_monotone_relaxed
    monotone_order = vertical_monotone_order_constant

    ! Run subroutine to test
    f_interp_test = f_interp
    call monotone_quintic_sl(f_interp_test,f_grid,sc,cl,monotone,monotone_order,1,6)

    ! Test the interpolated values
    @assertEqual(4.0_r_tran, f_interp_test(1), tol)
    @assertEqual(3.1_r_tran, f_interp_test(2), tol)
    @assertEqual(4.526656_r_tran, f_interp_test(3), tol)
    @assertEqual(1.858_r_tran, f_interp_test(4), tol)
    @assertEqual(1.0_r_tran, f_interp_test(5), tol)
    @assertEqual(1.0_r_tran, f_interp_test(6), tol)

    ! Test relaxed monotone and linear order -------------------------
    monotone = vertical_monotone_relaxed
    monotone_order = vertical_monotone_order_linear

    ! Run subroutine to test
    f_interp_test = f_interp
    call monotone_quintic_sl(f_interp_test,f_grid,sc,cl,monotone,monotone_order,1,6)

    ! Test the interpolated values
    @assertEqual(4.0_r_tran, f_interp_test(1), tol)
    @assertEqual(3.1_r_tran, f_interp_test(2), tol)
    @assertEqual(4.526656_r_tran, f_interp_test(3), tol)
    @assertEqual(1.858_r_tran, f_interp_test(4), tol)
    @assertEqual(1.0_r_tran, f_interp_test(5), tol)
    @assertEqual(1.0_r_tran, f_interp_test(6), tol)

    ! Test relaxed monotone and high order -------------------------
    monotone = vertical_monotone_relaxed
    monotone_order = vertical_monotone_order_high

    ! Run subroutine to test
    f_interp_test = f_interp
    call monotone_quintic_sl(f_interp_test,f_grid,sc,cl,monotone,monotone_order,1,6)

    ! Test the interpolated values
    @assertEqual(4.0_r_tran, f_interp_test(1), tol)
    @assertEqual(3.1_r_tran, f_interp_test(2), tol)
    @assertEqual(4.526656_r_tran, f_interp_test(3), tol)
    @assertEqual(1.858_r_tran, f_interp_test(4), tol)
    @assertEqual(1.0_r_tran, f_interp_test(5), tol)
    @assertEqual(1.0_r_tran, f_interp_test(6), tol)

  end subroutine monotone_quintic_sl_test

