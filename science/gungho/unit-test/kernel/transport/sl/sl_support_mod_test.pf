  @test
  subroutine compute_cubic_hermite_coeffs_test()
    use funit
    use, intrinsic :: iso_fortran_env,  only: real64
    use sl_support_mod,                 only: compute_cubic_hermite_coeffs

    implicit none

    real(kind=r_tran)   :: z_grid(1:5), z_interp(1:5), dz(1:5), tol
    real(kind=r_tran)   :: cc(1:4,1:5)
    integer(kind=i_def) :: sc(1:4,1:5)
    integer(kind=i_def) :: ni, ng

    ! Set tolerance
    if ( r_tran == real64 ) then
      tol = 1.0e-12_r_tran
    else
      tol = 1.0e-6_r_tran
    end if

    ! Parameters
    ni = 5_i_def
    ng = 5_i_def

    ! Set up cell centres on uniform grid
    dz(1:5)=1.0_r_tran
    z_grid(1) = 0.5_r_tran
    z_grid(2) = 1.5_r_tran
    z_grid(3) = 2.5_r_tran
    z_grid(4) = 3.5_r_tran
    z_grid(5) = 4.5_r_tran

    ! Set interpolated location
    z_interp(1) = 0.5_r_tran
    z_interp(2) = 0.8_r_tran
    z_interp(3) = 2.5_r_tran
    z_interp(4) = 3.3_r_tran
    z_interp(5) = 4.5_r_tran

    ! Run subroutine
    call compute_cubic_hermite_coeffs(z_interp,z_grid,dz,sc,cc,ni,ng)

    ! Test the indices
    @assertEqual(1.0_r_tran, real(sc(1,1),r_tran), tol)
    @assertEqual(1.0_r_tran, real(sc(2,1),r_tran), tol)
    @assertEqual(2.0_r_tran, real(sc(3,1),r_tran), tol)
    @assertEqual(3.0_r_tran, real(sc(4,1),r_tran), tol)
    @assertEqual(1.0_r_tran, real(sc(1,2),r_tran), tol)
    @assertEqual(1.0_r_tran, real(sc(2,2),r_tran), tol)
    @assertEqual(2.0_r_tran, real(sc(3,2),r_tran), tol)
    @assertEqual(3.0_r_tran, real(sc(4,2),r_tran), tol)
    @assertEqual(1.0_r_tran, real(sc(1,3),r_tran), tol)
    @assertEqual(2.0_r_tran, real(sc(2,3),r_tran), tol)
    @assertEqual(3.0_r_tran, real(sc(3,3),r_tran), tol)
    @assertEqual(4.0_r_tran, real(sc(4,3),r_tran), tol)
    @assertEqual(2.0_r_tran, real(sc(1,4),r_tran), tol)
    @assertEqual(3.0_r_tran, real(sc(2,4),r_tran), tol)
    @assertEqual(4.0_r_tran, real(sc(3,4),r_tran), tol)
    @assertEqual(5.0_r_tran, real(sc(4,4),r_tran), tol)
    @assertEqual(3.0_r_tran, real(sc(1,5),r_tran), tol)
    @assertEqual(4.0_r_tran, real(sc(2,5),r_tran), tol)
    @assertEqual(5.0_r_tran, real(sc(3,5),r_tran), tol)
    @assertEqual(5.0_r_tran, real(sc(4,5),r_tran), tol)

    ! Test the cubic Hermite coefficients
    @assertEqual(0.0_r_tran, cc(1,1), tol)
    @assertEqual(1.0_r_tran, cc(2,1), tol)
    @assertEqual(0.0_r_tran, cc(3,1), tol)
    @assertEqual(0.0_r_tran, cc(4,1), tol)
    @assertEqual(0.0_r_tran,     cc(1,2), tol)
    @assertEqual(0.6685_r_tran,  cc(2,2), tol)
    @assertEqual(0.363_r_tran,   cc(3,2), tol)
    @assertEqual(-0.0315_r_tran, cc(4,2), tol)
    @assertEqual(0.0_r_tran, cc(1,3), tol)
    @assertEqual(0.0_r_tran, cc(2,3), tol)
    @assertEqual(1.0_r_tran, cc(3,3), tol)
    @assertEqual(0.0_r_tran, cc(4,3), tol)
    @assertEqual(-0.016_r_tran, cc(1,4), tol)
    @assertEqual(0.168_r_tran,  cc(2,4), tol)
    @assertEqual(0.912_r_tran,  cc(3,4), tol)
    @assertEqual(-0.064_r_tran, cc(4,4), tol)
    @assertEqual(0.0_r_tran, cc(1,5), tol)
    @assertEqual(0.0_r_tran, cc(2,5), tol)
    @assertEqual(1.0_r_tran, cc(3,5), tol)
    @assertEqual(0.0_r_tran, cc(4,5), tol)

  end subroutine compute_cubic_hermite_coeffs_test

